<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <com.github.cchao.MoneyView
            android:id="@+id/moneyView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:money_text="2352.00"
            app:prefix_text="￥" />
        <com.lany.priceview.PriceView
            app:value_text="25365.000"
            app:prefix_text="￥"
            app:prefix_size="15sp"
            app:prefix_color="#ff9900"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
     <net.yhkj.mvvmdemo.view.OriginalPriceView
         android:layout_width="wrap_content"
         app:prefix_text="￥"
         app:prefix_size="15sp"
         app:value_text="25365.000"
         android:layout_height="wrap_content"/>
    </LinearLayout>
</layout>